	"scripts": {
		"build:dev": "webpack watch --node-env=development",
		"build:prod": "webpack --node-env=production",
		"build:debug": "webpack --node-env=development",
		"start:watch": "nodemon dist/app.js",
		"lint": "eslint src/*",
		"test": "jest",
		"mysqldump:full": "docker exec -t mysql mysqldump -u root --databases post-restaurant-reviews > ./sql/full.sql",
		"mysqldump:schema": "docker exec -t mysql mysqldump -u root --databases post-restaurant-reviews --no-data | sed -e 's/ AUTO_INCREMENT=[0-9]*//' > ./sql/1_schema.sql",
		"mysqldump:data": "docker exec -t mysql mysqldump -u root -t --databases post-restaurant-reviews --no-create-db > ./sql/2_data.sql"
	}
